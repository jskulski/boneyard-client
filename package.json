{
  "devDependencies": {
    "babel": "^5.6.5",
    "chai": "^3.0.0",
    "jsdom": "3.*",
    "mocha": "^2.2.5",
    "parallelshell": "^1.1.1",
    "sinon": "^1.15.3",
    "testdom": "^1.0.1",
    "webpack-dev-server": "^1.9.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "private": true,
  "scripts": {
    "dist": "npm run-script clean && npm run-script build",
    "serve:prod": "npm run-script dist& npm run-script start:prod",
    "serve:dev": "npm run-script dist& npm run-script start:dev",
    "clean": "rm -rf dist",
    "build": "webpack -d -w --content-base build/",
    "start:prod": "node server.js",
    "start:dev": "nodemon server.js",
    "test": "mocha --compilers js:babel/register --recursive"
  },
  "dependencies": {
    "express": "^4.13.0",
    "flux": "^2.0.3",
    "functional.js": "^0.6.16",
    "http-proxy": "^1.11.1",
    "jsx-loader": "^0.13.2",
    "nanoajax": "^0.3.1",
    "proxy-middleware": "^0.13.0",
    "react": "^0.13.3",
    "request": "^2.58.0",
    "webpack": "^1.9.12"
  }
}
